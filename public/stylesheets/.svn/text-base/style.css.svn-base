/* Sorting */
#content_full table.tablesorter {
	font-family: Georgia, "Times New Roman", Times, serif;
	font-size: 12px;
	font-weight: normal;
	background-color: #CDCDCD;
	margin: 10px 0pt 15px 0px;
	width: 100%;
	text-align: left;
}

table.tablesorter thead tr th, table.tablesorter tfoot tr th {
	background-color: #e6EEEE;
	border: 1px solid #FFF;
	font-size: 8pt;
	padding: 5px 0px 5px 7px;
}

table.tablesorter thead tr .header {
	background-color: #cab3b3;
	cursor: pointer;
	font-size: 13px;
	font-weight: normal;
	border: 1px #cab3b3 solid;
	color: #7e1223;
	text-align: left;
}

table.tablesorter tbody td {
	color: #3D3D3D;
	padding: 4px;
	background-color: #FFF;
	vertical-align: top;
	border: 1px #cab3b3 solid;
}

table.tablesorter tbody tr.odd td {
	background-color:#F0F0F6;
}

table.tablesorter thead tr .headerSortUp {
	background: url(../images/asc.gif) no-repeat right;
}

table.tablesorter thead tr .headerSortDown {
	background: url(../images/desc.gif) no-repeat right;
}

table.tablesorter thead tr .headerSortDown, table.tablesorter thead tr .headerSortUp {
	background-color: #770e1f;
	color: #fff;
}

#content_full .errorpage_extention {
	height: 300px;
}


