}

#slideshow {
	width: 392px;
	height: 375px;
	margin-left: 28px;
	display:inline;
}

#slideshow img {
	position:absolute;
    top:0;
    left:0;
    z-index:8;
	opacity:0.0;
}

#slideshow img.active {
    z-index:10;
	opacity:1.0;
	padding-top: 3px;
}

#slideshow img.last-active {
    z-index:9;
}

#slideshow_caption span {
	position:absolute;
    top:380px;
    left:0;
	opacity:0.0;
	width: 230px;
	font-size: 11px;
	color: #444;
	font-style: italic;
	margin: 5px 0px 0px 101px;
	background: none;
}

#slideshow_caption span.active {
	opacity:1.0;
}


