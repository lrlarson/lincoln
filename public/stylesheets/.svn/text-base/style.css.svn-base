/* General Styles
=================================*/

* {
	margin: 0px;
	padding: 0px;
}

body {
	font-family: Georgia, "Times New Roman", Times, serif;
	color: #373737;
	font-size: 12px;
	line-height: 15px;
	margin: 0 auto;
	background: #2d2d2d url(../images/bg.gif) repeat-x;
}

a:link {
	color: #901c2f;
	text-decoration: none;
	outline:none;
}

a:visited {
	color: #901c2f;
	text-decoration: none;
}

a:hover {
	color: none;
	background-color: transparent;
	text-decoration: underline;
}

.bodybold {	
	font-weight: bold;
}

.bodyunderline {	
	text-decoration: underline;
}

.bodyitalic {	
	font-style: italic;
}

.clear {
	clear:both;
}


/* Layout
=================================*/

#wrapper {
	width: 968px;
	margin: 0 auto;	
	background: #fff url(../images/bg_body.jpg) repeat-y;
}

/* Main Navigtion Styles */

.main_nav {
	width: 960px;
	height: 36px;	
	background: url(../images/bg_nav.gif) repeat-x;
}

.main_nav ul {
	font-family: Georgia, "Times New Roman", Times, serif;
	list-style-type: none;
	z-index: 10;
}

.main_nav ul li {
	float: left;
	color: #ffffff;	
	position: relative;	
	z-index: 100;
	outline: none;
}

.main_nav ul li a {
	font-family: Georgia, "Times New Roman", Times, serif;
	background: url(../images/nav_divider.jpg) no-repeat right;
	padding: 0px 20px;
	line-height: 36px;
	text-decoration: none;
	font-size: 13px;
	color: #ffffff;
	display: block;
	outline: none;
}

.main_nav ul li.current {
	background: url(../images/nav_divider.jpg) no-repeat right;
	background-color: #6f091a;
	padding: 0px 20px;
	line-height: 36px;
	text-decoration: none;
	color: #fff;
	font-size: 13px;
	display: block;
	outline: none;
}

.main_nav ul li.current_dropdown {
	background: none;
	background-color: #6f091a;
}

.main_nav ul a:hover {
	background-color: #6f091a;
	padding: 0px 20px;
	text-decoration: none;
	color: #fff;
	display: block;
	outline: none;
}

.main_nav ul a:visited {
	font-family: Georgia, "Times New Roman", Times, serif;
	background: url(../images/nav_divider.jpg) no-repeat right;
	padding: 0px 20px;
	line-height: 36px;
	text-decoration: none;
	font-size: 13px;
	color: #ffffff;
	display: block;
	outline: none;
}

.main_nav ul li.first_divider {
	background: url(../images/nav_divider.jpg) no-repeat right;
	padding: 0px 24px;
	line-height: 36px;
	text-decoration: none;
	color: #fff;
	display: block;
	outline: none;
} 

.main_nav li:hover ul {
	left: auto;
}

.main_nav li:hover ul, .main_nav li.sfhover ul {
	left: auto;
}

.main_nav li:hover > a  {
	background-color: #6f091a;
    cursor: hand;
	color:#ffffff;
}

*html .main_nav ul li a:hover {
	background-color: #6f091a;
	color:#ffffff;
}

/* Bibliography Dropdown */
.main_nav li ul {
	background-image: none;
	position: absolute;
	width: 189px;
	left: -999em;
	z-index: 100;
	border-top: 1px #6f091a solid;
}

.main_nav li ul a {
	width: 160px;
	height: 18px;
	background-color: #8e1b2e;
	font-size: 13px;
	line-height: 17px;
    padding: 5px 7px 5px 22px;
    margin: 0px 0px 0px 0px;
	color: #ffffff; 
	text-align: left;
    border-bottom: 1px solid #a83548;
 	outline: none;
	z-index: 100;
}

.main_nav li ul a:hover {
	line-height: 17px;
    padding: 5px 7px 5px 22px;
}

.main_nav li ul a:visited {
	width: 160px;
	height: 18px;
	background-color: #8e1b2e;
	font-size: 13px;
	line-height: 17px;
    padding: 5px 7px 5px 22px;
    margin: 0px 0px 0px 0px;
	color: #ffffff; 
	text-align: left;
    border-bottom: 1px solid #a83548;
 	outline: none;
}

.main_nav li ul li.double a {
	width: 164px;
	height: 18px;
	background-color: #8e1b2e;
	font-size: 13px;
	line-height: 17px;
    padding: 5px 3px 22px 22px;
	margin: 0px 0px 0px 0px;
	color: #ffffff; 
	text-align: left;
	border-bottom: 1px solid #a83548;
	outline: none;
}

.main_nav li ul li.double a:hover {
	line-height: 17px;
    padding: 5px 3px 22px 22px;
	background-color: #6f091a;
	text-decoration: none;
	color: #fff;
	display: block;
	outline: none;
}

.main_nav li ul li.double a:visited {
	width: 164px;
	height: 18px;
	background-color: #8e1b2e;
	font-size: 13px;
	line-height: 17px;
    padding: 5px 3px 22px 22px;
	margin: 0px 0px 0px 0px;
	color: #ffffff; 
	text-align: left;
	border-bottom: 1px solid #a83548;
	outline: none;
}


.main_nav li ul li.double a:visited:hover {
	background-color: #6f091a;
	line-height: 17px;
    padding: 5px 3px 22px 22px;
	text-decoration: none;
	color: #fff;
	display: block;
	outline: none;
}

#mainbody_wrapper {
	background: url(../images/bg_gradient_bottom.gif) no-repeat left bottom;	
	width: 959px;
	border-left: 1px #bababa solid;
	overflow: hidden;
 	position: relative;
	z-index: 1;
}

#mainbody_wrapper a:link {
	color: #901c2f;
	text-decoration: none;
	outline:none;
}

#mainbody_wrapper a:visited {
	color: #901c2f;
	text-decoration: none;
}

#mainbody_wrapper a:hover {
	color: none;
	background-color: transparent;
	text-decoration: underline;
}


/* Home Page Styles
=================================*/

#top_gradient_home {
