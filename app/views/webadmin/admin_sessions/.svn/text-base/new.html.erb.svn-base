<div id="content_full">
	<h2>Webadmin Login</h2>
	<% form_for [:webadmin, @user_session] do |f| %>
 	<div id="errorExplanation"><p><%= f.error_messages %></p></div>
		<div id="admin_login">
			<fieldset>
				<label><%= f.label :username, "User Name" %></label>
				<%= f.text_field :username %>
				<label><%= f.label :password, "Password" %></label>			
				<%= f.password_field :password %>
		 		<%= f.submit "Login", :id => "login" %>				
			</fieldset>
		</div>
<% end %>
</div>